{
  "name": "typescript-graphql-mongoose",
  "version": "0.0.1",
  "description": "Simple app using nodejs, graphql, mongodb",
  "author": "Carlos Alexandre Fattor <carlos.fattor@gmail.com>",
  "keywords": [
    "nodejs",
    "graphql",
    "express",
    "mongodb",
    "mongoose",
    "Javascript",
    "graphql-yoga"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "gen-schema-types": "ts-node src/scripts/createTypes.ts",
    "test": "NODE_ENV=test jest --watch"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.1",
    "ioredis": "^4.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.7.5",
    "mongoose-unique-validator": "^2.0.3",
    "nodemon": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "sparkpost": "^2.1.3",
    "typegoose": "^5.9.0",
    "uuid": "^3.3.2",
    "yup": "^0.27.0",
    "@gql2ts/from-schema": "^1.8.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/ioredis": "^4.0.14",
    "@types/jest": "^24.0.16",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.5.11",
    "@types/mongoose-unique-validator": "^1.0.2",
    "@types/node": "^12.6.9",
    "@types/sparkpost": "^2.1.4",
    "@types/uuid": "^3.4.5",
    "@types/yup": "^0.26.22",
    "@types/express": "^4.11.1",
    "gql2ts": "^1.10.1",
    "graphql-request": "^1.8.2",
    "jest": "^24.8.0",
    "node-fetch": "^2.6.0",
    "ts-jest": "^24.0.2",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
